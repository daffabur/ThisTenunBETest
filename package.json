{
  "name": "outfitsantara-backend",
  "version": "1.0.0",
  "description": "Backend API for Outfit Santara project (Express + Prisma)",
  "type": "module",
  "main": "src/server.js",
  "engines": { "node": ">=18.18" },
  "scripts": {
    "dev": "nodemon -r dotenv/config src/server.js",
    "start": "node src/server.js",

    "migrate:dev": "prisma migrate dev",
    "migrate:deploy": "prisma migrate deploy",

    "prisma:generate": "prisma generate",
    "prisma:studio": "prisma studio",

    "seed": "node prisma/seed.js",
    "seed:inspo": "node prisma/seed_inspo.js",
    "seed:articles": "node prisma/seed_articles.js",
    "seed:all": "npm run seed && npm run seed:inspo && npm run seed:articles",

    "postinstall": "prisma generate"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@prisma/client": "^6.15.0",
    "cors": "^2.8.5",
    "dotenv": "^17.2.1",
    "express": "^5.1.0",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "nodemon": "^3.1.10",
    "prisma": "^6.15.0"
  }
}
